<script>
    $(document).ready(function () {
        $.ajax({
            url: '@Url.Action("GetDashboard")',
            success: function (result) {
                var HtmlDash = "";
                $(result).each(function () {
                    HtmlDash += "<label class='col-lg-2 col-md-6 col-sm-6 col-xs-12'>\
                                    <div class='dash-icon-bkg'>\
                                        <a href='" + this.Url + "' class='text-secondary' style='text-decoration:none;'>\
                                            <div class='col-lg-12 title'>" + this.Descricao + "</div>\
                                        </a>\
                                        <div class='col-lg-6 counter'><strong id='dash-top-count-1'>" + this.Quantidade + "</strong></div>\
                                    </div>\
                                </label>";
                });

                $("#divDashboard").html(HtmlDash);
            }
        });
    });

</script>

<div class="col m-3">
    <h3 class="title">
        Totalizador
    </h3>
    <div>
        <div id="divDashboard" class="bg-light home-dash-top">
        </div>
    </div>
</div>
